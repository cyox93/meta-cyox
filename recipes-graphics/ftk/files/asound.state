			type INTEGER
			count 1
			range '0 - 3'
		}
	}
	control.8 {
		iface MIXER
		name '3D effect'
		value Off
		comment {
			access 'read write'
			type ENUMERATED
			count 1
			item.0 Off
			item.1 Low
			item.2 Medium
			item.3 High
		}
	}
	control.9 {
		iface MIXER
		name 'HP Mux'
